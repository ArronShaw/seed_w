## Generic Build Instructions

cd ./seed_w
make clean
make

##For seed_w
cd src/algorithms/
make seed_w

##For test.c
gcc -o test test.c
./test

cd ../../examples_seed_w/01_one_word
./run.sh

cd ../../examples_seed_w/02_two_words
./run.sh

cd ../../examples_seed_w/03_three_words
./run.sh

cd ../../examples_seed_w/04_long_seqs
./run.sh

cd ../../examples_seed_w/05_HSL3
./run.sh
